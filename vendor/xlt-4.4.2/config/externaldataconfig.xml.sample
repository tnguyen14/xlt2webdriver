<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<config>
	
	<files>
		<!--
			File Parsers:
			
			com.xceptance.xlt.api.report.external.SimpleCsvParser
			Process simple CSV file.
			Value name is column index number.
			
			com.xceptance.xlt.api.report.external.HeadedCsvParser
			Process headed CSV file.
			Value names are taken from 1st line of CSV file.
			
			For more parsers see package com.xceptance.xlt.api.report.external.
		 -->
		<file source="my/path/data.csv" encoding="UTF-8" parserClass="com.xceptance.xlt.api.report.external.SimpleCsvParser">
		
			<headline>My CPU</headline>			
			<description>My visualization of logged CPU data</description>
						
			<charts>
			
				<chart title="My CPU usage" yAxisTitle="Usage in %" yAxisTitle2="Temperature in °C">
					<seriesCollection>
						<series valueName="1" title="System" color="#FF0000" average="10" averageColor="#840000" />
						<series valueName="2" title="Idle" color="#00FF00" average="10" averageColor="#008400" />
						<series valueName="3" title="User" color="#0000FF" average="10" averageColor="#000084" />
						<series valueName="6" title="Temp" axis="2" color="##FF7F24" />
					</seriesCollection>
				</chart>
				
				<!-- maximal chart config -->
				<!-- 
				<chart title="Chart title" xAxisTitle="X axis title" yAxisTitle="Left Y axis title" yAxisTitle2="Right Y axis title">
					<seriesCollection>
						<series valueName="1" title="Series title 1" axis="1" color="#00FF00" average="10" averageColor="#008400"/>
						<series valueName="2" title="Series title 2" axis="2" color="#FF0000" average="10" averageColor="#840000"/>
					</seriesCollection>
				</chart>
				-->
				
				<!-- minimal chart config -->
				<!-- 
				<chart title="My chart title">
					<seriesCollection>
						<series valueName="1" />
					</seriesCollection>
				</chart>
				-->
				
			</charts>
			
			<tables>
			
				<table title="My CPU statistics" type="minmaxavg">
					<rows>
						<row valueName="1" title="System" unit="%" />
						<row valueName="2" title="Idle" unit="%" />
						<row valueName="3" title="User" unit="%" />
						<row valueName="6" title="Temp" unit="°C" />
					</rows>
				</table>
				
				<!-- maximal table config -->
				<!-- 
				<table title="Network Statistics" type="minmaxavg">
					<rows>
						<row valueName="1" title="Series title 1" description="Series description 1" unit="%" />
						<row valueName="2" title="Series title 2" description="Series description 2" unit="MHz" />
					</rows>
				</table>
				-->
				
				<!-- minimal table config -->
				<!-- 
				<table title="Table title">
					<rows>
						<row valueName="1" />
					</rows>
				</table>
				-->
				
			</tables>
			
			<properties>
				<property key="parser.dateFormat.pattern" value="dd.MM.yyyy HH:mm:ss" />
				<property key="parser.dateFormat.timeZone" value="GMT+0" />
				<property key="parser.csv.separator" value="," />
				<!--
					Use this line for "Tab" separators.
					<property key="parser.csv.separator" value="&#x9;" />
				
					<property key="someOtherKey" value="someValue" />
				-->
			</properties>
			
		</file>
	
	</files>
	
</config>